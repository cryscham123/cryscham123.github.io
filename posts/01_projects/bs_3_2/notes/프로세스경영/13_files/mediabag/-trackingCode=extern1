                if (materialLink) {
                    e.preventDefault();
                    var linkData = materialLink.getAttribute("data-linkdata");
                    if (linkData) {
                        try {
                            var parsedData = JSON.parse(linkData);
                            if (typeof materialResolver === "function") {
                                materialResolver(parsedData);
                            }
                        } catch (err) {
                        }
                    }
                }
            });
        });
