<script>
    
    var currentToken;
    function callbackFromFESignUp(event) {
        var data = event.data;

        if(event.origin != window.location.origin){
            return;
        }

        if(data.token != currentToken){
            return;
        }

        data.refresh && location.reload();
    }

    window.addEventListener('message', callbackFromFESignUp, false);
</script>


