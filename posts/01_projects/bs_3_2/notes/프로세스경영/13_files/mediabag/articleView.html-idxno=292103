/* ë‚ ê°œë°°ë„ˆ ìŠ¤í¬ë¦½íŠ¸  */
/* ****************************************** */
$('#wingBanner .wing-banner-left .wing-banner').each(function(){
	if( $(this).data('scroll') == 'y' ) $(this).addClass('scroll-on');
});

$(window).scroll(function() {
	if( $('#wingBanner .wing-banner').length > 0 ){											// ë‚ ê°œë°°ë„ˆê°€ ìˆìœ¼ë©´ ì‘ë™

		var scrollingHeight_L = nonScrollingHeight_L = scrollingHeight_R = nonScrollingHeight_R = 0;

		$('#wingBanner .wing-banner').each(function(idx){
			var $_this		= $(this),														// ë‚ ê°œë°°ë„ˆ
				scroll		= $_this.data('scroll'),										// ë‚ ê°œë°°ë„ˆ ìŠ¤í¬ë¡¤ ì—¬ë¶€
				topMargin	= $_this.data('top-margin'),									// ë‚ ê°œë°°ë„ˆ ìƒë‹¨ì—¬ë°±
				downMargin	= $_this.data('down-margin'),									// ë‚ ê°œë°°ë„ˆ í•˜ë‹¨ì—¬ë°±
				duration	= $_this.data('duration'),										// ë‚ ê°œë°°ë„ˆ ìŠ¤í¬ë¡¤ ì†ë„
				direction	= $_this.data('direction'),										// ë‚ ê°œë°°ë„ˆ ìœ„ì¹˜
				// bannerTop	= $_this.offset().top,											// ë‚ ê°œë°°ë„ˆ ìƒë‹¨ ìœ„ì¹˜ (ê°€ë³€)
				bannderHeight = $_this.outerHeight();										// ë‚ ê°œë°°ë„ˆ ë†’ì´ (ê³ ì •)

			var fixWingBannertop	= $('#wingBanner').offset().top;						// ë‚ ê°œë°°ë„ˆ ìƒë‹¨ ìœ„ì¹˜ (ê³ ì •)

			var articleTitleHeight	= $('#sticky-header.scroll').height();			// ë‚ ê°œë°°ë„ˆ ìŠ¤í¬ë¡¤ ì‹œ ìƒë‹¨ íƒ€ì´í‹€ ë†’ì´ (ê¸°ì‚¬ ë³¸ë¬¸ ì‹œ)
			if(!articleTitleHeight) articleTitleHeight = 0;

			var $_userFooter		= $('#user-footer'),									// ì‚¬ì´íŠ¸ Footer
				userFooterHeight	= $_userFooter.outerHeight();							// ì‚¬ì´íŠ¸ Footer ë†’ì´ (ê³ ì •)

			var winScrollTop		= $(window).scrollTop(),								// ìœˆë„ìš° ìŠ¤í¬ë¡¤ ìœ„ì¹˜ (ê°€ë³€)
				winHeight			= $(window).height(),									// ìœˆë„ìš° ë†’ì´
				docHeight			= $(document).height();									// í˜ì´ì§€ ë†’ì´

			switch (direction) {															// ë‚ ê°œ ë°°ë„ˆ ìœ„ì¹˜ ë³„
				case 'left':
					if(scroll == 'y') scrollingHeight_L += bannderHeight;					// ìˆœì„œëŒ€ë¡œ ë…¸ì¶œë˜ëŠ” ë‚ ê°œë°°ë„ˆ ì¤‘ ìŠ¤í¬ë¡¤ í•˜ëŠ” ë°°ë„ˆì˜ ë†’ì´ê°’ (ì¢Œ)
					else nonScrollingHeight_L += bannderHeight;								// ìˆœì„œëŒ€ë¡œ ë…¸ì¶œë˜ëŠ” ë‚ ê°œë°°ë„ˆ ì¤‘ ìŠ¤í¬ë¡¤ í•˜ì§€ ì•ŠëŠ” ë°°ë„ˆì˜ ë†’ì´ê°’ (ì¢Œ)
					break;

				case 'right':
					if(scroll == 'y') scrollingHeight_R += bannderHeight;					// ìˆœì„œëŒ€ë¡œ ë…¸ì¶œë˜ëŠ” ë‚ ê°œë°°ë„ˆ ì¤‘ ìŠ¤í¬ë¡¤ í•˜ëŠ” ë°°ë„ˆì˜ ë†’ì´ê°’ (ìš°)
					else nonScrollingHeight_R += bannderHeight;								// ìˆœì„œëŒ€ë¡œ ë…¸ì¶œë˜ëŠ” ë‚ ê°œë°°ë„ˆ ì¤‘ ìŠ¤í¬ë¡¤ í•˜ì§€ ì•ŠëŠ” ë°°ë„ˆì˜ ë†’ì´ê°’ (ìš°)
					break;
			}

			if(scroll == 'y') {																// ìŠ¤í¬ë¡¤ ë°°ë„ˆë§Œ ì›€ì§ì„ ì²˜ë¦¬
				var bannerAllHeight = siteAllHeight = 0;

				// ì¢Œìš°ì¸¡ ê°ê° ë°°ë„ˆ ìŠ¤í¬ë¡¤ ì²˜ë¦¬ë¥¼ ìœ„í•´ ë‚˜ëˆ ì„œ ì²˜ë¦¬
				if(direction == 'left'){																			// ì¢Œì¸¡ ë‚ ê°œ
					var wingBnTop_L = winScrollTop - fixWingBannertop + topMargin - nonScrollingHeight_L;			// ë‚ ê°œë°°ë„ˆ ìŠ¤í¬ë¡¤ ìœ„ì¹˜ (ê°€ë³€)

					if( articleTitleHeight ) wingBnTop_L += articleTitleHeight;										// ê¸°ì‚¬ë³¸ë¬¸ ì‹œ ìƒë‹¨ ìŠ¤í¬ë¡¤ íƒ€ì´í‹€ì´ ë‚˜íƒ€ë‚˜ë©´ ìŠ¤í¬ë¡¤ íƒ€ì´í‹€ ë§Œí¼ ë‚ ê°œë°°ë„ˆ ìœ„ì¹˜ ì¡°ì •

					if((fixWingBannertop + nonScrollingHeight_L) < winScrollTop) {

						bannerAllHeight = articleTitleHeight + scrollingHeight_L + 1;								// ìƒë‹¨ íƒ€ì´í‹€ + ìŠ¤í¬ë¡¤ë°°ë„ˆì˜ ì´ ë†’ì´
						siteAllHeight = winHeight - userFooterHeight + downMargin;									// ìœˆë„ìš° ë†’ì´ + ì‚¬ì´íŠ¸ Footer ë†’ì´

						if( (winScrollTop >= ((docHeight - userFooterHeight) - winHeight)) && (bannerAllHeight >= siteAllHeight) ) {					// ì‚¬ì´íŠ¸ Footer ë„˜ì–´ê°€ëŠ” ê²½ìš° ì²˜ë¦¬
							wingBnTop_L -= ((userFooterHeight + downMargin) - (winHeight - (scrollingHeight_L + topMargin + articleTitleHeight)));
							$('#wingBanner .wing-banner-left .wing-banner.scroll-on').animate({top:wingBnTop_L+"px" },{queue: false, duration: duration});		// ë§ˆì§€ë§‰ ë°°ë„ˆ ì›€ì§ì„ ì²˜ë¦¬
						}
					} else {
						wingBnTop_L = 0;																			// ìµœìƒë‹¨ ì²˜ë¦¬
					}
					$_this.animate({top:wingBnTop_L+"px" },{queue: false, duration: duration});

				} else {																							// ìš°ì¸¡ ë‚ ê°œ

					var wingBnTop_R = winScrollTop - fixWingBannertop + topMargin - nonScrollingHeight_R;			// ë‚ ê°œë°°ë„ˆ ìŠ¤í¬ë¡¤ ìœ„ì¹˜ (ê°€ë³€)

					if( articleTitleHeight ) wingBnTop_R += articleTitleHeight;										// ê¸°ì‚¬ë³¸ë¬¸ ì‹œ ìƒë‹¨ ìŠ¤í¬ë¡¤ íƒ€ì´í‹€ì´ ë‚˜íƒ€ë‚˜ë©´ ìŠ¤í¬ë¡¤ íƒ€ì´í‹€ ë§Œí¼ ë‚ ê°œë°°ë„ˆ ìœ„ì¹˜ ì¡°ì •

					if((fixWingBannertop + nonScrollingHeight_R) < winScrollTop) {

						bannerAllHeight = articleTitleHeight + scrollingHeight_R + 1;								// ìƒë‹¨ íƒ€ì´í‹€ + ìŠ¤í¬ë¡¤ë°°ë„ˆì˜ ì´ ë†’ì´
						siteAllHeight = winHeight - userFooterHeight + downMargin;									// ìœˆë„ìš° ë†’ì´ + ì‚¬ì´íŠ¸ Footer ë†’ì´

						if( (winScrollTop >= ((docHeight - userFooterHeight) - winHeight)) && (bannerAllHeight >= siteAllHeight) ) {					// ì‚¬ì´íŠ¸ Footer ë„˜ì–´ê°€ëŠ” ê²½ìš° ì²˜ë¦¬
							wingBnTop_R -= ((userFooterHeight + downMargin) - (winHeight - (scrollingHeight_R + topMargin + articleTitleHeight)));
							$('#wingBanner .wing-banner-right .wing-banner.scroll-on').animate({top:wingBnTop_R+"px" },{queue: false, duration: duration});		// ë§ˆì§€ë§‰ ë°°ë„ˆ ì›€ì§ì„ ì²˜ë¦¬
						}
					} else {
						wingBnTop_R = 0;																			// ìµœìƒë‹¨ ì²˜ë¦¬
					}
					$_this.animate({top:wingBnTop_R+"px" },{queue: false, duration: duration});
				}
			}
		});
	}

});
</script>


</body>
